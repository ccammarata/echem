electrode1Ch1 <- data1[Ch2in,6]-data1[Ch1in,6]
electrode1Ch2 <- data1[Ch3in,6]-data1[Ch2in,6]
electrode1Ch3 <- data1[H2O2in,6]-data1[Ch3in,6]
