slopeMat1 <- matrix(data=NA,nrow = 4,ncol = 2)
colnames(slopeMat1) <- c('uM','pA')
slopeMat1[,1]<-c(0,20,40,60)
slopeMat1[,2]<-c(data1[Ch1in,6],data1[Ch2in,6],data1[Ch3in,6],data1[H2O2in,6])
